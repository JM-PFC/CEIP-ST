{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Horario list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Horaclase</th>
                <th>Inicio</th>
                <th>Fin</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('horario_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.horaClase }}</td>
                <td>{{ entity.inicio }}</td>
                <td>{{ entity.fin }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('horario_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('horario_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('horario_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
