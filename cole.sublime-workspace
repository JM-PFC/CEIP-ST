o{\n	border-top: 1px solid black;\n}\n.contenido_info .columna_foto{\n	width: 96%;\n}\n.contenido_info .columna_foto img{\n	display: block;\n	margin: 5px auto;\n	margin-bottom: 10px;\n}\n.contenido_info #seleccionado ul{\n	padding: 0;\n	margin: 0 0 0 30px;\n}\n.contenido_info #seleccionado li{\n	color: #334c73;\n	padding:0;\n	font-size: 15px;\n	margin: 4px 0;\n	margin-bottom: 5px;\n}\n.contenido_info #seleccionado h3{\n	padding: 0 10%;\n	margin: 0 0;\n	margin-top: 5px;\n	clear: left;\n}\n.contenido_info #sin_seleccionar p{\n	text-align: center;\n	font-size: 1.2em;\n	font-family: Courier,Helvetica,Arial,sans-serif;\n	margin-top: 17vh;\n}\n#multiples_alumnos #sin_seleccionar p{\n	margin-top: 14vh;;\n}\n.no_cursor{\n	pointer-events: none !important ;\n	cursor: none !important;	\n}\n\n.old_t_data h3{\n	padding: 0 2% !important;\n	float: left;\n	font-size: 1.2em;\n}\n.old_t_data span{\n	float: left;\n	margin-left: 2%;\n	color: #334c73;\n	margin-top: 5px;\n	font-size: 1.2em;\n}\n\n#consulta_reservas_instalaciones .scrollContent tr[class*=\"no_cursor\"],#consulta_reservas_equipamientos .scrollContent tr[class*=\"no_cursor\"]{\n	pointer-events: none !important ;\n	cursor: none !important;\n}\n#consulta_reservas_instalaciones .scrollContent tr[class*=\"no_cursor\"] td,#consulta_reservas_equipamientos .scrollContent tr[class*=\"no_cursor\"] td{\n	pointer-events: none !important;\n	cursor: none !important;\n}\n.scrollContent tr td img,.scrollContent tr td img{\n	cursor: pointer; \n	pointer-events: auto;\n	margin: 0 -3% 0 7%;\n}\n.scrollContent .dataTables_empty{\n	padding-top: 18vh !important;\n}\n.swal-wide{\n\n    width:600px !important;\n    margin-left: -317px !important;\n}\n.sweet-alert button:focus{\n	text-decoration: underline;\n\n}\n.sweet-alert th, .sweet-alert td{\n	font-size: 15px !important;\n	text-align: center;\n}\n .sweet-alert td{\n	color: #334c73;\n	min-width: 85px;\n}\n .sweet-alert td:nth-child(1){\n	text-align: left;\n}\n .sweet-alert table{\n	margin:0 aut